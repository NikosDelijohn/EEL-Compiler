/* + ----------------- [ I/O ] ----------------- + *
 * + A very simple Input Output example.         + *
 * + Program asks for an integer and prints it.  + *
 */

program IO

    input a;    //0.Asks for a number
    print a     //1.Prints the number

endprogram      //2.Halts

